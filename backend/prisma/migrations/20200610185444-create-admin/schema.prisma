// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Admin{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  whatsapp String? @unique
  latitude Float?
  longitude Float?
}

